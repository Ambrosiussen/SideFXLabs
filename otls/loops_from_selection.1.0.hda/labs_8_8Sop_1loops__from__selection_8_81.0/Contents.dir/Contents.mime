--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/select_quad_loop.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"detail"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"int max_iteration = chi(\"max_iteration\");

if (max_iteration < 1) {
    return;
}

int iteration = 0;

int start_pt0 = point(1, \"__index\", 0);
int start_pt1 = point(1, \"__index\", 1);

string group_name = chs(\"group_name\");
int create_group_per_loop = chi(\"create_group_per_loop\");
int edge_index = point(1, \"__edge_index\", 0);

int quads_only = chi(\"quads_only\");

int start_hedge0 = pointhedge(0, start_pt0, start_pt1);
int start_hedge1 = pointhedge(0, start_pt1, start_pt0);

int hedge = start_hedge0;
int prim;

// Should keep going: 0
// Current primitive is not a quad: -1
// Next hedge is the start hedge: 1
// Max iteration is reached: 2
int termination_code = 0;

// Searches in the first direction
do {

    prim = hedge_prim(0, hedge);
    setprimgroup(0, group_name, prim, 1, \"set\");

    if (create_group_per_loop) {
        setprimgroup(0, group_name + itoa(edge_index), prim, 1, \"set\");
    }

    if (len(primpoints(0, prim)) != 4) {

        termination_code = -1;

        if (quads_only) {

            setprimgroup(0, group_name, prim, 0, \"set\");

            if (create_group_per_loop) {
                setprimgroup(0, group_name + itoa(edge_index), prim, 0, \"set\");
            }
        }

    } else {

        hedge = hedge_next(0, hedge_next(0, hedge));
        hedge = hedge_nextequiv(0, hedge);

        if (hedge == start_hedge0) {
            termination_code = 1;
        }
    }

    iteration ++;

    if (termination_code == 0 && iteration >= max_iteration) {
        termination_code = 2;
    }

} while (termination_code == 0);


if (termination_code == -1 || termination_code == 2) {

    hedge = start_hedge1;
    iteration = 0;
    termination_code = 0;

    // Searches in the second direction
    do {

        prim = hedge_prim(0, hedge);
        setprimgroup(0, group_name, prim, 1, \"set\");

        if (create_group_per_loop) {
            setprimgroup(0, group_name + itoa(edge_index), prim, 1, \"set\");
        }

        if (len(primpoints(0, prim)) != 4) {

            termination_code = -1;

            if (quads_only) {

                setprimgroup(0, group_name, prim, 0, \"set\");

                if (create_group_per_loop) {
                    setprimgroup(0, group_name + itoa(edge_index), prim, 0, \"set\");
                }
            }

        } else {

            hedge = hedge_next(0, hedge_next(0, hedge));
            hedge = hedge_nextequiv(0, hedge);

            if (hedge == start_hedge1) {
                termination_code = 1;
            }
        }

        iteration ++;

        if (termination_code == 0 && iteration >= max_iteration) {
            termination_code = 2;
        }

    } while (termination_code == 0);
}


// DEBUG ONLY
//i@termination_code = termination_code;
//i@iterations = iteration;"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"on"	)
bindings	[ 0	locks=0 ]	(	0	)
groupautobind	[ 0	locks=0 ]	(	"on"	)
groupbindings	[ 0	locks=0 ]	(	0	)
vex_cwdpath	[ 0	locks=0 ]	(	.	)
vex_outputmask	[ 0	locks=0 ]	(	*	)
vex_updatenmls	[ 0	locks=0 ]	(	"off"	)
vex_matchattrib	[ 0	locks=0 ]	(	id	)
vex_inplace	[ 0	locks=0 ]	(	"off"	)
vex_selectiongroup	[ 0	locks=0 ]	(	""	)
vex_precision	[ 0	locks=0 ]	(	auto	)
folder1	[ 0	locks=0 ]	(	0	0	)
max_iteration	[ 0	locks=0 ]	(	[ max_iteration	0 ] 	)
create_group_per_loop	[ 0	locks=0 ]	(	[ create_group_per_loop	0 ] 	)
quads_only	[ 0	locks=0 ]	(	[ quads_only	0 ] 	)
group_name	[ 0	locks=0 ]	(	[ group_name	"" ] 	)
}

