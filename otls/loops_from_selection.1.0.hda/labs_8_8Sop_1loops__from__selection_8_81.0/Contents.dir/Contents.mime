--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/select_edge_loop.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"detail"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"int max_iteration = chi(\"max_iteration\");

if (max_iteration < 1) {
    return;
}

int iteration = 0;

int start_pt0 = point(1, \"__index\", 0);
int start_pt1 = point(1, \"__index\", 1);

string group_name = chs(\"group_name\");
int create_group_per_loop = chi(\"create_group_per_loop\");
int edge_index = point(1, \"__edge_index\", 0);

setedgegroup(0, group_name, start_pt0, start_pt1, 1);

if (create_group_per_loop) {
    setedgegroup(0, group_name + itoa(edge_index), start_pt0, start_pt1, 1);
}

int prev_point = start_pt1;
int this_point = start_pt0;
int next_point = -1;

int neighbour_pts[];
int parallel_prims[];
int parallel_pts[];

// Should keep going: 0
// There is not one and only one non-parallel neighbour point: -1
// Next point is the other start point: 1
// Max iteration is reached: 2
int termination_code = 0;

// Searches in the first direction
while (termination_code == 0) {

    // Builds the parallel points array

    parallel_prims = pointprims(0, prev_point);
    parallel_pts = {};

    foreach (int prim; parallel_prims) {
        append(parallel_pts, primpoints(0, prim));
    }

    // Loops through each neighbour point to see
    // if there is one and only one non-parallel next point

    neighbour_pts = neighbours(0, this_point);
    int count = 0;

    foreach (int pt; neighbour_pts) {

        // Checks if this neighbour point is
        // in the parallel points array

        int in_parallel_pts = 0;

        foreach (int parallel_pt; parallel_pts) {

            if (parallel_pt == pt) {

                in_parallel_pts = 1;
                break;
            }
        }

        if (in_parallel_pts == 0) {

            next_point = pt;
            count ++;
        }
    }

    if (count == 1) {

        setedgegroup(0, group_name, this_point, next_point, 1);

        if (create_group_per_loop) {
            setedgegroup(0, group_name + itoa(edge_index), this_point, next_point, 1);
        }

        prev_point = this_point;
        this_point = next_point;

        if (next_point == start_pt1) {
            termination_code = 1;
        }

    } else {

        termination_code = -1;
    }

    iteration ++;

    if (termination_code == 0 && iteration >= max_iteration) {
        termination_code = 2;
    }
}


if (termination_code == -1 || termination_code == 2) {

    prev_point = start_pt0;
    this_point = start_pt1;
    next_point = -1;
    iteration = 0;
    termination_code = 0;

    // Searches in the second direction
    while (termination_code == 0) {

        // Builds the parallel points array

        parallel_prims = pointprims(0, prev_point);
        parallel_pts = {};

        foreach (int prim; parallel_prims) {
            append(parallel_pts, primpoints(0, prim));
        }

        // Loops through each neighbour point to see
        // if there is one and only one non-parallel next point

        neighbour_pts = neighbours(0, this_point);
        int count = 0;

        foreach (int pt; neighbour_pts) {

            // Checks if this neighbour point is
            // in the parallel points array

            int in_parallel_pts = 0;

            foreach (int parallel_pt; parallel_pts) {

                if (parallel_pt == pt) {

                    in_parallel_pts = 1;
                    break;
                }
            }

            if (in_parallel_pts == 0) {

                next_point = pt;
                count ++;
            }
        }

        if (count == 1) {

            setedgegroup(0, group_name, this_point, next_point, 1);

            if (create_group_per_loop) {
                setedgegroup(0, group_name + itoa(edge_index), this_point, next_point, 1);
            }

            prev_point = this_point;
            this_point = next_point;

            if (next_point == start_pt0) {
                termination_code = 1;
            }

        } else {

            termination_code = -1;
        }

        iteration ++;

        if (termination_code == 0 && iteration >= max_iteration) {
            termination_code = 2;
        }
    }
}


// DEBUG ONLY
//i@termination_code = termination_code;
//i@iterations = iteration;"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"on"	)
bindings	[ 0	locks=0 ]	(	0	)
groupautobind	[ 0	locks=0 ]	(	"on"	)
groupbindings	[ 0	locks=0 ]	(	0	)
vex_cwdpath	[ 0	locks=0 ]	(	.	)
vex_outputmask	[ 0	locks=0 ]	(	*	)
vex_updatenmls	[ 0	locks=0 ]	(	"off"	)
vex_matchattrib	[ 0	locks=0 ]	(	id	)
vex_inplace	[ 0	locks=0 ]	(	"off"	)
vex_selectiongroup	[ 0	locks=0 ]	(	""	)
vex_precision	[ 0	locks=0 ]	(	auto	)
folder1	[ 0	locks=0 ]	(	0	0	)
max_iteration	[ 0	locks=0 ]	(	[ max_iteration	0 ] 	)
create_group_per_loop	[ 0	locks=0 ]	(	[ create_group_per_loop	0 ] 	)
group_name	[ 0	locks=0 ]	(	[ group_name	"" ] 	)
}

