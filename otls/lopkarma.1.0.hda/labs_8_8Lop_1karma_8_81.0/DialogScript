                default { "-" }
                hidewhen "{ enablemplaymonitor == 0 }"
                menutoggle {
                    [ "node = kwargs['node'].node(\"karmarenderproperties\")" ]
                    [ "kwargs['node'] = node" ]
                    [ "planes = node.hm().getPlanes(kwargs)" ]
                    [ "return planes + [\"-\", \"All AOVs\"]" ]
                    language python
                }
                parmtag { "script_callback_language" "python" }
            }
            parm {
                name    "previewscale"
