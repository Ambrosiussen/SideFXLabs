{
    "path": "$SIDEFXLABS",
    "load_package_once": true,
    "enable": "houdini_version >= '19.0' and houdini_version < '19.1'",
    "version": "MANUAL",
    "env": [
        {
            "SIDEFXLABS": "$HOUDINI_PACKAGE_PATH/SideFXLabs18.5"
        },
        {
            "PATH": {
                "method": "prepend",
                "value": [
                    "$SIDEFXLABS/bin"
                ]
            }
        }
    ]
}
