{
    "env": [
        {
            "sidefxlabs_current_version": "MANUAL"
        }, 
        {
            "SIDEFXLABS": "Your/Path/To/The/Toolset"
        }, 
        {
            "PATH": {
                "method": "prepend", 
                "value": [
                    "$SIDEFXLABS/bin"
                ]
            }
        }
    ], 
    "path": "$SIDEFXLABS"
}