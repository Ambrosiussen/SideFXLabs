{
    "path": "$SIDEFXLABS",
    "enable": "houdini_version >= '18.0' and houdini_version < '18.1'",
    "version": "MANUAL",
    "env": [
        {
            "SIDEFXLABS": "$HOUDINI_PACKAGE_PATH/SideFXLabs18.0"
        },
        {
            "PATH": {
                "method": "prepend",
                "value": [
                    "$SIDEFXLABS/bin"
                ]
            }
        }
    ]
}
